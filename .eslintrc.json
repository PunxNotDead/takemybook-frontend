{
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": 7,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "comma-dangle": 0,
        "no-tabs": 0,
        "require-jsdoc": 0,
        "max-len": 0,
        "arrow-parens": 0,
        "object-curly-spacing": 0,
        "react/jsx-uses-react": "error",
        "react/jsx-uses-vars": "error"
    },
    "parser": "babel-eslint",
    "plugins": [
        "react"
    ]
}